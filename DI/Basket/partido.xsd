<?xml version="1.0" encoding="UTF-8" ?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema">
    <xs:element name="partido">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="equipo" maxOccurs="2" minOccurs="2">
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element name="nombre" type="xs:string"/>
                            <xs:element name="jugador" maxOccurs="7" minOccurs="5">
                                <xs:complexType>
                                    <xs:sequence>
                                        <xs:element name="nombre" type="xs:string"/>
                                        <xs:element name="dorsal" type="xs:integer"/>
                                    </xs:sequence>
                                    <xs:attribute name="titular" type="xs:string" use="required"/>
                                </xs:complexType>
                            </xs:element>
                        </xs:sequence>
                    </xs:complexType>
                </xs:element>
                <xs:element name="destacados">
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element name="falta" minOccurs="0">
                                <xs:complexType>
                                    <xs:sequence>
                                        <xs:element name="equipo" type="xs:string"/>
                                        <xs:element name="nombre_jugador" type="xs:string"/>
                                        <xs:element name="dorsal" type="xs:integer"/>
                                    </xs:sequence>
                                </xs:complexType>
                            </xs:element>
                            <xs:element name="anotacion" minOccurs="0">
                                <xs:complexType>
                                    <xs:sequence>
                                        <xs:element name="equipo" type="xs:string"/>
                                        <xs:element name="nombre_jugador" type="xs:string"/>
                                        <xs:element name="dorsal" type="xs:integer"/>
                                        <xs:element name="puntuacion" type="xs:byte"/>
                                    </xs:sequence>
                                </xs:complexType>
                            </xs:element>
                        </xs:sequence>
                    </xs:complexType>
                </xs:element>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
</xs:schema>

